//判断一个数是不是奇数
int main()
{
	    int a = 0;
		scanf("%d",&a);

		if (a % 2 == 0)
			printf("a不为奇数\n");
		else
			printf("a为奇数\n");
	return 0;

}


//判断1-100是不是奇数，并打印
int main()
{
	int i = 1;
	while (i <= 100)
	{
		if (i% 2 == 0)
			printf("%d不为奇数\n",i);
		else
			printf("%d为奇数\n",i);
		i++;
	}

	return 0;

}


//输出1---100的的奇数
int main()
{
	int i = 1;
	while (i <= 100)
	{
		printf("%d ", i);
		i += 2;
	}
	return 0;
}
